CREATE TABLE TIME SLOT(
time_slot_id VARCHAR(4) PRIMARY KEY,
day CHAR(1),
start_time DATE,
end_time DATE
);

INSERT INTO TIME_SLOT (time_slot_id, day, start_time, end_time) VALUES ('F','M',TO_DATE('14:00','HH24:MI'),TO_DATE('15:30','HH24:MI'));
