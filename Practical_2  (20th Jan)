CREATE TABLE TEACHES(
instructor_id INT,
section_id INT,
PRIMARY KEY (instructor_id, section_id),
FOREIGN KEY (instructor_id) REFERENCES INSTRUCTOR(instructor_id),
FOREIGN KEY (section_id) REFERENCES SECTION (section_id)
);

