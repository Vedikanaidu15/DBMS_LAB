CREATE TABLE SECTION(
section_id INT PRIMARY KEY,
course_id VARCHAR(8),
sec_id INT,
semester VARCHAR(6),
year INT,
building VARCHAR(50),
room_number VARCHAR(10),
time_slot_id VARCHAR(4),
FOREIGN KEY (course_id) REFERENCES COURSE(course_id)
);

INSERT INTO SECTION (section_id, course_id, sec_id, semester, year, building, room_number, time_slot_id) VALUES (1,'CS-101',1,'Fall',2023,'Talyor','3128','A');
