CREATE TABLE TAKES(
student_id INT,
section_id INT,
grade VARCHAR(2),
PRIMARY KEY (student_id_, section_id),
FOREIGN KEY (student_id) REFERENCES INSTRUCTOR(student_id),
FOREIGN KEY (section_id) REFERENCES SECTION (section_id)
);

INSERT INTO TAKES (student_id, section_id, grade) VALUES (1,1,'A');
